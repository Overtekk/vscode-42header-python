{
  "name": "42header-python",
  "displayName": "42 Header for Python",
  "description": "42 Header for Python (flake8 friendly) for VSCode : www.42.fr",
  "version": "1.1.0",
  "publisher": "Overtek",
  "icon": "42.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Overtekk/vscode-42header-python"
  },
  "bugs": {
    "url": "https://github.com/Overtekk/vscode-42header-python/issues"
  },
  "homepage": "https://github.com/Overtekk/vscode-42header-python",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "42headerpython.username": {
          "type": "string",
          "description": "Username to use in 42 headers-python"
        },
        "42headerpython.email": {
          "type": "string",
          "description": "Email address to use in 42 headers-python"
        },
		"42headerpython.customAuthor": {
          "type": "string",
		  "scope": "resource",
          "description": "Custom author name for group projects (e.g., 'login1, login2'). Overrides the default 'user <email>' format in the 'By:' field."
        }
      }
    },
    "commands": [
      {
        "command": "42headerpython.insertHeader",
        "title": "Insert 42 header",
        "when": "editorTextFocus"
      }
    ],
    "keybindings": [
      {
        "command": "42headerpython.insertHeader",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "@types/vscode": "^1.80.0"
  },
  "dependencies": {
    "moment": "^2.15.0"
  }
}
